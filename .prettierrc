{
  "semi": true,
  "printWidth": 120,
  "tabWidth": 4,
  "singleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "useTabs": false,
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "always",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "endOfLine": "auto",
  "singleAttributePerLine": false
}
